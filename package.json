{
  "name": "asamt-mobile",
  "version": "1.0.10",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint src",
    "precommit": "lint-staged",
    "run:android": "react-native run-android && adb reverse tcp:9090 tcp:9090",
